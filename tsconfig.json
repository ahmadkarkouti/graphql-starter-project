{
  "compilerOptions": {
    "lib": ["es6", "dom", "esnext.asynciterable"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "noImplicitReturns": false,
    "outDir": "lib",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es2020",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "compileOnSave": true,
  "include": ["src"]
}
